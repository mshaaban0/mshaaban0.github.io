.filters-top{
	padding-left: 20px;
}
.filters-top span{
	display: inline-block;
	color: #428bca;
	cursor: pointer;
}
.filters-top .itemName{
	padding: 2px 4px 2px 4px;
	float: left;
	font-size: 12px;
}
.filters-top .itemDelete{
	color: #aaa;
	background: #d9ecf8;
	padding: 2px 4px 2px 0px;
	float: left;
	visibility: hidden;
	font-size: 12px;
}
.filters-top .itemDelete:hover{
	color: #428bca;
}
.filters-top .itemCon{
	border-radius: 3px;
	overflow: hidden;
}
.filters-top .itemCon:hover .itemDelete{
	visibility: visible;
}
.filters-top .itemCon:hover .itemName{
	background: #d9ecf8;
}
.sort{
	border-top: 1px solid #d1d1d1;
	color: #504f4f;
}
.sort label,.sort a{
	font-weight: normal;
	font-size: 12px;
}
.list1-con .image img{
	max-width: 30px;
}
.list1-con .image a{
	float: left;
	margin-left: 5px;
	display: inline-block;
	border: 1px solid #bebebe;
	border-radius: 5px;
	overflow: hidden;
	box-shadow: 0 1px 3px rgba(0,0,0,.15);
}
.list1-con li{
	border-top: 1px solid #eee;
	padding: 5px 0;
	overflow: hidden;
	min-height: 42px;
