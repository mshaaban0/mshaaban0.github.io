}
.list1-con .add-task {
	line-height: 42px;
	padding-left: 30px;
	border-bottom: 1px solid #ededed;
}
.list1-con .image input[type="checkbox"]{
	position: absolute;
	left: 4px;
	visibility: hidden;
}
.list1-con li:hover .image input[type="checkbox"]{
	visibility: visible;
}
.list1-con{
	list-style-type: none;
}
.jspVerticalBar{
	width: 6px;
}
.jspDrag{
