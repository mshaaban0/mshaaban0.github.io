}

.check-btn *{
	cursor: pointer;
}
.check-btn span{
	display: inline-block;
	border: 1px solid #d1d1d1;
	width: 10px;
	height: 10px;
}
.check-btn span.checked {
	background: #428bca;
	border: 2px solid #e1e1e1;
}
.check-btn input{
	position: absolute;
	margin: 0;
	display: none;
}
.filters-container .panel-title>a{
	color: #504f4f;
	font-size: 12px;
	padding-left: 5px;
}
.filters-container .panel-title>a:hover{
	background: #eee;
}
.filters-container .panel-title>a i{
	font-size: 7px;
	font-weight: normal;
	display: inline-block;
	margin-right: 5px;
