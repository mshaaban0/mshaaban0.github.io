}
.list1-con li:hover{
	background: #eee;
}
.list1-con .task-title{
	color: #2e2e2e;
	text-decoration: none;
	font-weight: bold;
	font-size: 12px;
}
.list1-con .task-title:hover{
	text-decoration: none;
	color: #504f4f;

}
.list1-con .task-description{
	font-size: 12px;
