}
.hide{
	display: none;
}
.advanced-filters{
	text-align: center;
	padding: 20px 0;
}
.jspContainer,.jspScrollable,.jspPane{
	width: 100% !important;
}
div:focus{
	box-shadow: none;
	border: none;
}
.center-header .dropdown-menu>li>a:hover{
	background: none;
}
.center-section-container{
	
}

.inline-block{
	display: inline-block;
}


@media (min-width: 768px){
	.navbar-right .dropdown-menu {
	right: 10px;
	left: auto;
	}
	.navbar-collapse .navbar-nav.top-controllers.navbar-left:first-child{
		margin-left: 0;
	}
}